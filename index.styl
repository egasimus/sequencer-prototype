body, html {
  margin: 0;
  background-color: #000;
  font-family: sans-serif;
  overflow: hidden;
  height: 100%;
  width: 100%;
}

.Container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
}

svg {
  width: 100%;
  height: 100%;
  background: #080808;
}

